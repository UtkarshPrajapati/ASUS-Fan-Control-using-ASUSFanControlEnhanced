[Version]
Signature = "$Windows NT$"
Class = System
ClassGUID = {4D36E97D-E325-11CE-BFC1-08002BE10318}
Provider = "ASUSTeK COMPUTER INC."
DriverVer = 10/29/2024,3.1.38.0
PnpLockDown = 1
CatalogFile.ntamd64 = asussci2.cat


[Manufacturer]
%MfgName% = ASUP, ntamd64.10.0...16299

[ASUP.ntamd64.10.0...16299]
%DeviceDesc1% = NO_DRV64, ACPI\ASUS2018

[NO_DRV64]
Include = machine.inf
Needs = NO_DRV
CopyFiles = PackageCopySoftwareManager        ;Software Manager service
CopyFiles = PackageCopySoftwareManagerLicense ;Software Manager service
CopyFiles = PackageCopyOptimization	      	  ;Optimization service
CopyFiles = PackageCopyDiagnosis	      	  ;System Diagnosis service
CopyFiles = PackageCopyAnalysis               ;Analysis service
CopyFiles = PackageCopyAnalysisLicense        ;Analysis service
CopyFiles = PackageCopyAppService             ;App service
CopyFiles = PackageCopySwitch                 ;Switch service

[NO_DRV64.Services]
Include = machine.inf
Needs = NO_DRV.Services
AddService = ATKWMIACPIIO, , Optimization_Drv_ServiceInstall							;Optimization driver
AddService = ASUSOptimization, 0x00000800, Optimization_Srv_ServiceInstall				;Optimization service
AddService = ASUSSystemDiagnosis, 0x00000800, SystemDiagnosisServiceInstall     		;System Diagnosis service
AddService = ASUSSystemAnalysis, 0x00000800, Analysis_ServiceInstall		     		;Analysis service
AddService = ASUSSoftwareManager, 0x00000800, SoftwareManager_Usersrv_ServiceInstall    ;Software Manager service
AddService = AsusAppService, 0x00000800, AppService_ServiceInstall    					;App service
AddService = ASUSSwitch, 0x00000800, Switch_Srv_ServiceInstall							;Switch service

[DestinationDirs]
DefaultDestDir 		    		  = 13
PackageCopySoftwareManager        = 13,%SoftwareManagerFolder%    			;Software Manager service
PackageCopySoftwareManagerLicense = 13,%AsusSoftwareManagerLicenseFolder%   ;Software Manager service
PackageCopyOptimization	          = 13,%OptimizationFolder%       			;Optimization service
PackageCopyDiagnosis 	          = 13,%SystemDiagnosisFolder%				;System Diagnosis Service
PackageCopyAnalysis 			  = 13,%AnalysisFolder%						;Analysis service
PackageCopyAnalysisLicense 		  = 13,%AnalysisLicenseFolder%				;Analysis service
PackageCopyAppService             = 13,%AppServiceFolder%            		;App service
PackageCopySwitch         		  = 13,%SwitchFolder%            			;Switch service

; ==============================================================
;                    Software Manager service
; ==============================================================
[PackageCopySoftwareManager]
AsusLiveUpdate.dll
AsusLiveUpdate.ini
AsusSetup.exe
AsusSetupEx.exe
AsusSoftwareManager.exe
AsusToolkit.exe
AsusBusinessIntelligenceCollection.dll
AsusBusinessIntelligenceTransfer.dll
AsusBusinessIntelligenceCPPLib.dll
AsusSoftwareManagerAgent.exe
AsusAD.Common.dll
AsusAD.Core.dll
AsusADServiceAgent.dll
protobuf-net.dll
AsusUpdateChecker.exe
Dapper.dll
Microsoft.Data.Sqlite.dll
SQLitePCLRaw.batteries_v2.dll
SQLitePCLRaw.core.dll
SQLitePCLRaw.provider.e_sqlite3.dll
SQLitePCLRaw.provider.dynamic_cdecl.dll
e_sqlite3.dll
System.Memory.dll
System.Runtime.CompilerServices.Unsafe.dll
AsusSupportService.exe
AsusLiveUpdate_RPCClientRT.dll
msvcp140.dll
vcruntime140.dll
vcruntime140_1.dll
AsusSignCheck.dll
bsitf.dll
bsitf.sys
WinFlash.exe

[PackageCopySoftwareManagerLicense]
LICENSE_libzip.txt
LICENSE_libzippp.txt
LICENSE_SWMGR_RapidJSON.txt
LICENSE_Dapper.txt
LICENSE_Newtonsoft_Json.txt

[SoftwareManager_Usersrv_ServiceInstall]
DisplayName    = "ASUS Software Manager"
ServiceType    = 0x00000010      ; SERVICE_WIN32_SHARE_PROCESS
StartType      = 2               ; SERVICE_DEMAND_START, SERVICE_AUTO_START ==> 2
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %13%\%SoftwareManagerFolder%\AsusSoftwareManager.exe


; ==============================================================
;                    Optimization service                         
; ==============================================================
[PackageCopyOptimization]
AsusHotkey.exe
AsusOptimizationStartupTask.exe
AsusOptimization.exe
AsusOSD.exe
AsusWmiAcpi.sys
AsusSplendid.exe
AsusSplendid.dll
AsusTru2Life.dll
AsusCCT.dll
AsusColorKernel.dll
AsusICM.dll
ASUS Optimization 36D18D69AFC3.xml
AsusWiFiSmartConnect.exe
AsusWiFiRangeboost.exe
AsusTaskFirst.exe

[Optimization_Drv_ServiceInstall]
DisplayName    = "ATKWMIACPI Driver"
ServiceBinary  = %13%\%OptimizationFolder%\AsusWmiAcpi.sys
ServiceType    = 1
StartType      = 1
ErrorControl   = 1

[Optimization_Srv_ServiceInstall]
DisplayName    = "ASUS Optimization"
ServiceBinary  = %13%\%OptimizationFolder%\AsusOptimization.exe
ServiceType    = 16
StartType      = 2
ErrorControl   = 1
LoadOrderGroup = ShellSvcGroup

[PackageCopyDiagnosis]
AsusSystemDiagnosis.exe

[SystemDiagnosisServiceInstall]
DisplayName    = "ASUS System Diagnosis"
ServiceType    = 0x00000010      ; SERVICE_WIN32_SHARE_PROCESS
StartType      = 2               ; SERVICE_DEMAND_START, SERVICE_AUTO_START ==> 2
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %13%\%SystemDiagnosisFolder%\AsusSystemDiagnosis.exe


; ==============================================================
;                    Analysis service                         
; ==============================================================
[PackageCopyAnalysis]
AsusSystemAnalysis.exe
AsusWinIO64.dll
AsusSAIO.sys
AsusVerifyJWT.exe
wlanCliDLL.dll

[PackageCopyAnalysisLicense]
LICENSE_nlohmann-json.txt

[Analysis_ServiceInstall]
DisplayName    = "ASUS System Analysis"
ServiceBinary  = %13%\%AnalysisFolder%\AsusSystemAnalysis.exe
ServiceType    = 16
StartType      = 2
ErrorControl   = 1

; ==============================================================
;                    App service                         
; ==============================================================
[PackageCopyAppService]
AsusAppService.exe
AsusWebFlowLib.dll
AsusJWTDecoder.exe
AsusVirtualKeyboardLauncher.exe

[AppService_ServiceInstall]
DisplayName    = "ASUS App Service"
ServiceType    = 0x00000010      	; SERVICE_WIN32_SHARE_PROCESS
StartType      = 2               	; SERVICE_DEMAND_START, SERVICE_AUTO_START ==> 2
ErrorControl   = 1               	; SERVICE_ERROR_NORMAL
ServiceBinary  = %13%\%AppServiceFolder%\AsusAppService.exe

; ==============================================================
;                    Switch service                         
; ==============================================================
[PackageCopySwitch]
AsusSwitch.exe
AsusSwitchAgent.exe
AsusSwitchExe.exe
AsusSwitchHelper.exe
AsusSwitchNet.exe
AsusSwitchNetMDNS.exe

[Switch_Srv_ServiceInstall]
DisplayName    = "ASUS Switch"
ServiceBinary  = %13%\%SwitchFolder%\AsusSwitch.exe
ServiceType    = 16
StartType      = 2
ErrorControl   = 1

